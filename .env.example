# نمونه فایل تنظیمات محیطی
# این فایل رو کپی کن به .env و مقادیر رو تغییر بده

# آدرس وبسایت
WEBSITE_URL=https://example.com

# اعداد برای وارد کردن (با کاما جدا کن)
INPUT_NUMBERS=123,456,789

# سلکتورهای فیلدهای ورودی (با کاما جدا کن)
INPUT_SELECTORS=#input1,#input2,#input3

# سلکتور دکمه سابمیت
SUBMIT_SELECTOR=button[type="submit"]

# سلکتورهای نتایج (با کاما جدا کن)
RESULT_SELECTORS=.result-1,.result-2

# مرورگر headless باشه؟ (true/false)
HEADLESS=false
